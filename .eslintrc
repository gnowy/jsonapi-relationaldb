{
  "extends": "airbnb/base",
  "rules": {
    "arrow-body-style": 0,
    "func-names": 0,
    "max-len": 0,
    "no-else-return": 0,
    "no-multi-assign": 0,
    "no-multi-spaces": 0,
    "no-param-reassign": 0,
    "no-throw-literal": 0,
    "no-var": 0,
    "no-underscore-dangle": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "vars-on-top": 0
  }
}
